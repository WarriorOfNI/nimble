{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base"
  ],
  "enabledManagers": ["npm"],
  "rangeStrategy": "update-lockfile",
  "packageRules": [
    {
      "matchManagers": ["npm"],
      "enabled": false
    },
    {
      "groupName": "beachball",
      "matchPackageNames": ["beachball"],
      "enabled": true
    },
    {
      "groupName": "FAST",
      "matchPackagePatterns": ["^@microsoft/fast"],
      "enabled": true
    },
    {
      "groupName": "NI ESLint config",
      "matchPackagePatterns": ["^@ni/eslint-config"],
      "enabled": true
    }
  ]
}
